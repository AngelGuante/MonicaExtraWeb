<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Monica Extra Web</title>

    @*  ICONOS  *@
    <link async href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    @*  BOOTSTRAP *@
    <link href="~/Content/css/Bootstrapt.css" rel="stylesheet">
    <link href="~/Content/css/ReportStyle.css" rel="stylesheet">
    <link href="~/Content/css/PrintStyle.css" rel="stylesheet">
    <link href="~/Content/css/SAP.css" rel="stylesheet">
</head>
<body>
    <div>
        <nav class="navbar navbar-dark bg-info dontPrint" style="display:flex; justify-content:flex-start; align-items:center">
            <img src="~/Content/assets/logoHeaderMonica.png" alt="Logo Monica" height="40" width="55" class="mr-2" />
            <label class="navbar-brand m-0"> <strong>MoniExtra Beta</strong> </label>
            <label class="navbar-brand d-none d-sm-block m-0"><strong> - Funciones Adicionales en el Sistema MONICA</strong></label>
        </nav>

        <div id="divMaster" class="container body-content">
            @RenderBody()
        </div>
    </div>

    <script src="~/Scripts/js/Utils.js"></script>

    @* VUE *@
    <script src="~/Scripts/js/Vue.js"></script>

    @* SWAL *@
    <script src="~/Scripts/js/Swal.js"></script>

    @* CHARTS *@
    <script src="~/Scripts/js/Chart.js"></script>

    <script async src="https://cdn.jsdelivr.net/npm/popper.js@1.16.1/dist/umd/popper.min.js" integrity="sha384-9/reFTGAW83EW2RDu2S0VKaIzap3H66lZH81PoYlFhbGU+6BZp6G7niu735Sk7lN" crossorigin="anonymous"></script>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
