<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>

    @*  ICONOS  *@
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">

    @*  BOOTSTRAP *@
    <link href="~/Content/css/Bootstrapt.css" rel="stylesheet">

    <link href="~/Content/css/ReportStyle.css" rel="stylesheet">

    <link href="~/Content/css/PrintStyle.css" rel="stylesheet">

</head>
<body>
    <div>
        <nav class="navbar navbar-dark bg-info dontPrint" style="display:flex; justify-content:flex-start">
            <label class="navbar-brand"> <strong>MONICAEXTRA</strong> </label>
            <label class="navbar-brand d-none d-sm-block"><strong> -   SISTEMA DE CAJA WEB</strong></label>
        </nav>

        <div id="divMaster" class="container body-content">
            @RenderBody()
        </div>
    </div>

    <script src="~/Scripts/js/Utils.js"></script>

    @* VUE *@
    <script src="~/Scripts/js/Vue.js"></script>

    @* SWAL *@
    <script src="~/Scripts/js/Swal.js"></script>

    @* CHARTS *@
    <script src="~/Scripts/js/Chart.js"></script>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
