<div id="CuadresDeCajaCRUD" style="margin-top:8px;" hidden>
    <button type="button" v-on:click="DetallesMovimientosParaCierreCaja()" class="btn btn-info Sombra" data-toggle="modal" data-target=".bd-example-modal-lg"> <i class="material-icons" style="margin-right:10px; vertical-align:bottom">close</i>Crear cierre </button>

    @*  MODAL PARA HACER CUADRES DIARIOS*@
    <div class="modal fade bd-example-modal-lg" id="modalCierreCaja" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" aria-hidden="true" data-backdrop="static" data-keyboard="false">
        <div class="modal-dialog modal-lg">
            <div class="modal-content">
                <div class="p-5">

                    <button type="button" class="btn btn-outline-dark" style="float:right;" onclick="$('#modalCierreCaja').modal('hide');"><i class="material-icons" style="margin-right:10px; vertical-align:bottom">close</i></button>

                    <h1>CREAR CIERRE DE CAJA</h1>

                    <hr />

                    <div class="row">
                        <div class="col-sm">
                            <div class="form-group">
                                <label for="fechaUltimoCierre">Fecha Inicial</label>
                                <input id="fechaUltimoCierre" type="date" style="width:100%" v-model="VentanaCierres.VentanaCrearCierreDiario.FechaInicial" class="form-control" disabled>
                            </div>
                        </div>

                        <div class="col-sm">
                            <div class="form-group">
                                <label for="fechaEmicion">Fecha Final</label>
                                <input type="date" style="width:100%" v-model="VentanaCierres.VentanaCrearCierreDiario.FechaFinal" class="form-control" disabled>
                            </div>
                        </div>

                        @*<a class="btn btn-light" style="text-align:center; vertical-align:central" v-on:click="CuadresCaja(0)"><i class="material-icons" style="margin-top:80%;">search</i></a>*@
                    </div>

                    <div class="card" style="margin-top:10px;">
                        <div class="card-body">
                            <div class="row">
                                <div class="col-sm">
                                    <h5 class="card-title">Saldo Final:</h5>
                                    <p class="card-text"><strong>{{VentanaCierres.VentanaCrearCierreDiario.SaldoFinal}}</strong></p>
                                </div>
                                <div class="col-sm">
                                    <h5 class="card-title">Numero de Movimientos:</h5>
                                    <p class="card-text"><strong>{{VentanaCierres.VentanaCrearCierreDiario.NumeroDeMovimientos}}</strong></p>
                                </div>
                            </div>
                        </div>

                        <button type="button" class="btn btn-danger" style="width:100%;" v-on:click="CerrarCaja()"> <i class="material-icons" style="margin-right:10px; vertical-align:bottom">close</i> Cerrar Caja</button>
                    </div>

                    <hr />

                    <button id="btnVerMovimientosParaCierre" v-on:click="MovimientosEnCierre()" onclick="this.setAttribute('hidden', true);" type="button" class="btn btn-info" style="width:100%;">
                        Ver Todos Los Movimientos De Esta Fecha <br />
                        <i class="material-icons">arrow_downward</i>
                    </button>

                    <hr />

                    <div id="todosMovimientosParaCierre" class="Tables" v-if="VentanaCierres.VentanaCrearCierreDiario.MovimientosParaCierre">
                        <table class="table table-striped">
                            <thead>
                                <tr>
                                    <th scope="col">Beneficiario</th>
                                    <th scope="col">Concepto</th>
                                    <th scope="col">Monto</th>
                                    <th scope="col">Fecha</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr v-for="movimiento in VentanaCierres.VentanaCrearCierreDiario.MovimientosParaCierre">
                                    <td>{{movimiento.nombre_completo}}</td>
                                    <td>{{movimiento.Concepto}}</td>
                                    <td>{{movimiento.Monto}}</td>
                                    <td>{{movimiento.Fecha}}</td>
                                </tr>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <hr />

    <div class="card border-info mb-3 mt-2 Sombra">
        <div class="card-body text-info">
            <div class="row">
                <div class="col-sm">
                    <div class="form-group">
                        <label for="fechaEmicion">Fecha Inicial</label>
                        <input type="date" style="width:100%" v-model="VentanaCierres.FechaInicial" class="form-control">
                    </div>
                </div>

                <div class="col-sm">
                    <div class="form-group">
                        <label for="fechaEmicion">Fecha Final</label>
                        <input type="date" style="width:100%" v-model="VentanaCierres.FechaFinal" class="form-control">
                    </div>
                </div>

                <a class="btn btn-light" style="text-align:center; vertical-align:central" v-on:click="CuadresCaja(0)"><i class="material-icons" style="margin-top:80%;">search</i></a>
            </div>
        </div>
    </div>

    @*<button class="btn btn-outline-info Sombra" style="width:100%;" type="button" onclick="printJS('toPrintCierres', 'html');"><i class="material-icons" style="vertical-align:bottom">print</i></button>*@

    <div class="Tables">
        <table class="table table-striped">
            <thead>
                <tr>
                    <th style="text-align:center;" scope="col">#</th>
                    <th scope="col">Fehca Inicial</th>
                    <th scope="col">Fecha Final</th>
                    <th style="text-align:center;" scope="col">Saldo Final</th>
                    <th scope="col"></th>
                </tr>
            </thead>
            <tbody>
                <tr v-for="element in VentanaCierres.Cierres">
                    <td style="text-align:center;">{{element.NumeroCierre}}</td>
                    <td>{{element.FechaInicial}}</td>
                    <td>{{element.FechaFinal}}</td>
                    <td style="text-align:center;">{{element.SaldoFinal}}</td>
                    <td style="text-align:center;"><button class="btn btn-outline-info Sombra" type="button" v-on:click="Print('cierre', { 'NumeroCierre': element.NumeroCierre }, {'NumeroCierre': element.NumeroCierre, 'FechaInicial': element.FechaInicial, 'FechaFinal': element.FechaFinal, 'SaldoFinal': element.SaldoFinal})"><i class="material-icons" style="vertical-align:bottom">print</i></button></td>
                </tr>
        </table>

        <hr />

        <nav aria-label="Page navigation example">
            <ul class="pagination justify-content-center">
                <li v-bind:class="{'page-item': true, disabled: VentanaCierres.PaginatorIndex == 1 ? true : false}">
                    <a class="page-link" v-on:click="CierresCajaPaginatorMovimientos(1)"><<</a>
                </li>
                <li v-bind:class="{'page-item': true, disabled: VentanaCierres.PaginatorIndex == 1 ? true : false}">
                    <a class="page-link" v-on:click="CierresCajaPaginatorMovimientos(VentanaCierres.PaginatorIndex - 1)"><</a>
                </li>
                <li class="page-item disabled">
                    <a class="page-link">{{VentanaCierres.PaginatorIndex}}</a>
                </li>
                <li v-bind:class="{'page-item':true, disabled: VentanaCierres.PaginatorIndex == VentanaCierres.PaginatorLastPage ? true : false}">
                    <a class="page-link" v-on:click="CierresCajaPaginatorMovimientos(VentanaCierres.PaginatorIndex + 1)">></a>
                </li>
                <li v-bind:class="{'page-item':true, disabled: VentanaCierres.PaginatorIndex == VentanaCierres.PaginatorLastPage ? true : false}">
                    <a class="page-link" v-on:click="CierresCajaPaginatorMovimientos(VentanaCierres.PaginatorLastPage)">>></a>
                </li>
            </ul>
        </nav>
    </div>
</div>